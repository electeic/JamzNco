<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="jamilaappinc.grubmate.RequestFragment">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <TextView
            android:id="@+id/textView2"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:text="Request Food"
            android:textAlignment="center"
            android:textSize="30dp" />

        <EditText
            android:id="@+id/titleText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="49dp"
            android:ems="10"
            android:text="Title of Post" />

        <EditText
            android:id="@+id/tagsText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="99dp"
            android:ems="10"
            android:text="Tags" />

        <EditText
            android:id="@+id/dietaryText"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="149dp"
            android:ems="10"
            android:text="Date" />

        <EditText
            android:id="@+id/StartText"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginTop="199dp"
            android:ems="10"
            android:text="Start Time" />

        <EditText
            android:id="@+id/EndText"
            android:layout_width="150dp"
            android:layout_height="wrap_content"
            android:ems="10"
            android:text="End Time"
            android:layout_alignBaseline="@+id/StartText"
            android:layout_alignBottom="@+id/StartText"
            android:layout_alignParentEnd="true"
            android:layout_marginEnd="14dp" />

        <Button
            android:id="@+id/button"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Submit"
            android:layout_alignBaseline="@+id/button2"
            android:layout_alignBottom="@+id/button2"
            android:layout_alignEnd="@+id/StartText" />

        <Button
            android:id="@+id/button2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Cancel"
            android:layout_marginTop="104dp"
            android:layout_below="@+id/EndText"
            android:layout_alignStart="@+id/EndText" />
        <android.support.design.widget.FloatingActionButton
            android:id="@+id/menu_from_main"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="bottom|end"
            android:src="@drawable/gmlogo"
            android:layout_marginLeft="30dp"
            android:layout_marginRight="30dp"
            android:layout_below="@+id/button2"
            android:layout_alignEnd="@+id/EndText"
            android:layout_marginTop="61dp" />

    </RelativeLayout>
</FrameLayout>