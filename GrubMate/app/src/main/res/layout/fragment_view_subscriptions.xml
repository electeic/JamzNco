<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="jamilaappinc.grubmate.ViewSubscriptionsFragment">

    <!-- List elements is made up of notification_info_view or notification_info_request_view
            depends on what kind of notification it is.
        -->
<RelativeLayout
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <!-- TODO: Update blank fragment layout -->

    <TextView
        android:id="@+id/viewSubscriptions_Title"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="23dp"
        android:text="My Subscriptions "
        android:textSize="26sp" />

    <ListView
        android:id="@+id/viewSubscriptions_list"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/viewSubscriptions_Title"
        android:layout_marginTop="35dp" />
</RelativeLayout>
</FrameLayout>